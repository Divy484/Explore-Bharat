<% if (locals.success && success.length > 0) { %>
    Swal.fire({
        toast: true,
        position: 'top-end',
        icon: 'success',
        title: '<%= success[0] %>',
        showConfirmButton: false,
        timer: 3000,
        timerProgressBar: true,
    });
<% } %>

<% if (locals.error && error.length > 0) { %>
    Swal.fire({
        toast: true,
        position: 'top-end',
        icon: 'error',
        title: '<%= error[0] %>',
        showConfirmButton: false,
        timer: 3000,
        timerProgressBar: true,
    });
<% } %>